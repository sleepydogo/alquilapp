
<% if (user_signed_in? && ( (current_user.rango == 'Usuario') || (current_user.rango == 'A_Verificar') || (current_user.rango == 'Bloqueado'))) %>
    <%= render 'devise/registrations/edit' %>
<% elsif %>
    <% if (user_signed_in? && current_user.rango == 'No_Aceptado') %>
        <%= render 'devise/registrations/editDocu' %>
    <% end %>
<% end %>
